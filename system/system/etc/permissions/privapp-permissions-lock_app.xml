<!--
This XML file declares which signature|privileged permissions should be granted to privileged
applications that come with the platform
-->
<permissions>
    <privapp-permissions package="com.rjio.lockunlock">
        <permission name="com.rjio.permission.JPNLOCK" />
        <permission name="android.permission.BLUETOOTH"/>
        <permission name="android.permission.BLUETOOTH_ADMIN"/>
        <permission name="android.permission.BLUETOOTH_CONNECT" />
        <permission name="android.permission.ACCESS_WIFI_STATE"/>
        <permission name="android.permission.CHANGE_WIFI_STATE"/>
        <permission name="android.permission.DISPATCH_PROVISIONING_MESSAGE"/>
        <permission name="android.permission.REBOOT"/>
        <permission name="android.permission.READ_PHONE_STATE"/>
        <permission name="android.permission.MANAGE_ROLE_HOLDERS"/>
        <permission name="android.permission.WRITE_SECURE_SETTINGS"/>
        <permission name="android.permission.CALL_PHONE" />
        <permission name="android.permission.MANAGE_OWN_CALLS" />
        <permission name="android.permission.MODIFY_PHONE_STATE"/>
    </privapp-permissions>

</permissions>
